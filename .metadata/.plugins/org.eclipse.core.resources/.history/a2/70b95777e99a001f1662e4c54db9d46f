
public class Rough {

	public static void main(String[] args) {
		
		int n=4;
		int m=5;
		for(int rows=1; rows<=n; rows++) {
			for(int cols=1; cols<=m; cols++) {
				if(cols==1 || cols==m) {
					System.out.println("* ");
				} else {
					System.out.println(" ");
				}
			}
			System.out.println();
		}
		
		
	}

}
